import json

# פתיחת הקובץ
with open('user_choices.json', 'r') as f:
    # קריאת כל התוכן של הקובץ
    data = f.readlines()

# הצגת התוכן של הקובץ
for line in data:
    # המרת כל שורה (מילון JSON) לאובייקט Python והדפסתו
    user_data = json.loads(line)
    print(user_data)
